#include <stdio.h>
#include <stdlib.h>
#include "xparameters.h"
#include "xgpio.h"

void delay(int dly)
{
    int i, j;
    for (i = 0; i < dly; i++) {
    	for (j = 0; j < 0xffff; j++) {
    		;
        }
    }
}

int main()
{
    XGpio LED_XGpio;
    int led_num = 0x03;

    XGpio_Initialize(&LED_XGpio, XPAR_AXI_GPIO_0_BASEADDR);
    XGpio_SetDataDirection(&LED_XGpio, 1, 0);

    printf("Start!!!");

    while(1) {
    	printf();

    }

    return 0;
}


